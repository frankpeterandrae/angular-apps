{
	"name": "@angular-apps/source",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "nx",
		"start": "nx run homepage:serve",
		"build": "nx exec -- nx build",
		"e2e": "ng e2e",
		"affected:apps": "nx affected:apps",
		"affected:libs": "nx affected:libs",
		"affected:build": "nx affected:build",
		"affected:e2e": "nx affected:e2e",
		"affected:test": "nx affected:test",
		"affected:lint": "nx affected:lint",
		"affected:dep-graph": "nx affected:dep-graph",
		"affected": "nx affected",
		"format:fix": "prettier --config .prettierrc --write \"{apps,libs,src,e2e}/**/*{.ts,.js,.json,.scss,.html,.php}\"",
		"update": "nx migrate latest",
		"dep-graph": "nx dep-graph",
		"help": "nx help",
		"prepare": "husky",
		"watch:shared-styles": "node libs/shared/ui-theme/scripts/watch-css.js",
		"serve:watch": "concurrently \"nx run start\" \"nx run shared-ui-theme:watch-css\"",
		"countlines": "cloc . --exclude-dir=.angular,.husky,.idea,.nx,.vscode,dist,node_modules,tmp,coverage --exclude-list-file=package-lock.json",
		"foss": "license-checker --json --production --customPath licenseFormat.json --out tmp/foss-licenses.json && node ./scripts/combine-licenses.js"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "18.2.8",
		"@angular/cdk": "18.2.9",
		"@angular/common": "18.2.8",
		"@angular/compiler": "18.2.8",
		"@angular/core": "18.2.8",
		"@angular/forms": "18.2.8",
		"@angular/platform-browser": "18.2.8",
		"@angular/platform-browser-dynamic": "18.2.8",
		"@angular/router": "18.2.8",
		"@jsverse/transloco": "^7.5.0",
		"@nx/angular": "20.1.2",
		"@push-based/ngx-fast-svg": "^18.1.0",
		"rxjs": "~7.8.1",
		"tslib": "^2.8.1",
		"zone.js": "~0.14.10"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^18.2.10",
		"@angular-devkit/core": "18.2.10",
		"@angular-devkit/schematics": "18.2.10",
		"@angular-eslint/eslint-plugin": "^18.4.0",
		"@angular-eslint/eslint-plugin-template": "^18.4.0",
		"@angular-eslint/template-parser": "^18.4.0",
		"@angular/cli": "18.2.10",
		"@angular/compiler-cli": "18.2.8",
		"@angular/language-service": "18.2.9",
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.15.0",
		"@nx/cypress": "20.1.2",
		"@nx/devkit": "20.1.2",
		"@nx/eslint": "20.1.2",
		"@nx/eslint-plugin": "20.1.2",
		"@nx/jest": "20.1.2",
		"@nx/js": "20.1.2",
		"@nx/web": "20.1.2",
		"@nx/workspace": "20.1.2",
		"@prettier/plugin-php": "^0.22.2",
		"@schematics/angular": "18.2.10",
		"@swc-node/register": "~1.10.9",
		"@swc/core": "~1.9.2",
		"@swc/helpers": "~0.5.15",
		"@types/jest": "^29.5.14",
		"@types/node": "22.9.0",
		"@typescript-eslint/eslint-plugin": "^8.14.0",
		"@typescript-eslint/parser": "^8.14.0",
		"@typescript-eslint/utils": "^8.14.0",
		"angular-eslint": "^18.4.0",
		"concurrently": "^9.1.0",
		"cypress": "^13.15.2",
		"eslint": "~9.14.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-cypress": "^4.1.0",
		"eslint-plugin-jsdoc": "^50.5.0",
		"husky": "^9.1.6",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-preset-angular": "~14.3.0",
		"license-checker": "^25.0.1",
		"lint-staged": "^15.2.10",
		"nx": "20.1.2",
		"nx-stylelint": "^18.0.0",
		"prettier": "^3.3.3",
		"sass": "^1.81.0",
		"sass-migrator": "^2.2.1",
		"shx": "^0.3.4",
		"stylelint": "^16.10.0",
		"stylelint-config-standard": "^36.0.1",
		"stylelint-config-standard-scss": "^13.1.0",
		"ts-jest": "^29.2.5",
		"ts-node": "10.9.2",
		"typescript": "5.5.4"
	},
	"nx": {
		"targets": {
			"build": {
				"outputs": [
					"{projectRoot}/./dist"
				]
			}
		}
	},
	"lint-staged": {
		"*": "nx affected:lint",
		"*.{css,scss}": [
			"stylelint --fix",
			"prettier --config .prettierrc --write"
		],
		"*.{js,jsx,ts}": [
			"node scripts/sort-exports.js",
			"eslint --fix",
			"prettier --config .prettierrc --write"
		],
		"*.{md,json,html,php}": [
			"prettier --config .prettierrc --write"
		]
	}
}
