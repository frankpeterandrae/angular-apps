{
	"name": "@angular-apps/source",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "nx",
		"start": "nx run homepage:serve",
		"build": "nx exec -- nx build",
		"e2e": "ng e2e",
		"affected:apps": "nx affected:apps",
		"affected:libs": "nx affected:libs",
		"affected:build": "nx affected:build",
		"affected:e2e": "nx affected:e2e",
		"affected:test": "nx affected:test",
		"affected:lint": "nx affected:lint",
		"affected:dep-graph": "nx affected:dep-graph",
		"affected": "nx affected",
		"format:fix": "prettier --config .prettierrc --write \"{apps,libs,src,e2e}/**/*{.ts,.js,.json,.scss,.html,.php}\"",
		"update": "nx migrate latest",
		"dep-graph": "nx dep-graph",
		"help": "nx help",
		"prepare": "husky",
		"watch:shared-styles": "node libs/shared/ui-theme/scripts/watch-css.js",
		"serve:watch": "concurrently \"nx run start\" \"nx run shared-ui-theme:watch-css\"",
		"countlines": "cloc . --exclude-dir=.angular,.husky,.idea,.nx,.vscode,dist,node_modules,tmp,coverage --exclude-list-file=package-lock.json",
		"foss": "license-checker --json --production --customPath licenseFormat.json --out tmp/foss-licenses.json && node ./scripts/combine-licenses.js"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "20.2.4",
		"@angular/cdk": "20.2.2",
		"@angular/common": "20.2.4",
		"@angular/compiler": "20.2.4",
		"@angular/core": "20.2.4",
		"@angular/forms": "20.2.4",
		"@angular/platform-browser": "20.2.4",
		"@angular/platform-browser-dynamic": "20.2.4",
		"@angular/router": "20.2.4",
		"@jsverse/transloco": "^7.5.0",
		"@nx/angular": "21.5.1",
		"@push-based/ngx-fast-svg": "^18.1.0",
		"rxjs": "~7.8.1",
		"tslib": "^2.8.1",
		"zone.js": "0.15.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^20.1.6",
		"@angular-devkit/core": "20.2.2",
		"@angular-devkit/schematics": "20.2.2",
		"@angular-eslint/eslint-plugin": "20.2.0",
		"@angular-eslint/eslint-plugin-template": "20.2.0",
		"@angular-eslint/schematics": "20.2.0",
		"@angular-eslint/template-parser": "20.2.0",
		"@angular/cli": "~20.2.0",
		"@angular/compiler-cli": "20.2.4",
		"@angular/language-service": "20.2.4",
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.15.0",
		"@nx/devkit": "21.5.1",
		"@nx/eslint": "21.5.1",
		"@nx/eslint-plugin": "21.5.1",
		"@nx/jest": "21.5.1",
		"@nx/js": "21.5.1",
		"@nx/web": "21.5.1",
		"@nx/workspace": "21.5.1",
		"@prettier/plugin-php": "^0.22.2",
		"@schematics/angular": "20.2.2",
		"@swc-node/register": "1.9.2",
		"@swc/core": "1.5.7",
		"@swc/helpers": "0.5.17",
		"@types/jest": "30.0.0",
		"@types/node": "18.19.99",
		"@typescript-eslint/eslint-plugin": "8.43.0",
		"@typescript-eslint/parser": "8.43.0",
		"@typescript-eslint/utils": "8.43.0",
		"angular-eslint": "20.2.0",
		"concurrently": "^9.1.0",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-cypress": "^4.1.0",
		"eslint-plugin-jsdoc": "^50.5.0",
		"husky": "^9.1.7",
		"jest": "30.0.5",
		"jest-environment-jsdom": "30.0.5",
		"jest-preset-angular": "15.0.0",
		"jest-util": "30.0.5",
		"license-checker": "^25.0.1",
		"lint-staged": "^15.2.10",
		"nx": "21.5.1",
		"nx-stylelint": "^18.0.0",
		"prettier": "^3.5.3",
		"sass": "^1.81.0",
		"sass-migrator": "^2.2.1",
		"shx": "^0.3.4",
		"stylelint": "^16.10.0",
		"stylelint-config-standard": "^36.0.1",
		"stylelint-config-standard-scss": "^13.1.0",
		"ts-jest": "29.4.1",
		"ts-node": "10.9.2",
		"typescript": "5.9.2",
		"typescript-eslint": "8.43.0"
	},
	"nx": {
		"targets": {
			"build": {
				"outputs": [
					"{projectRoot}/./dist"
				]
			}
		}
	},
	"lint-staged": {
		"*": "nx affected:lint",
		"*.{css,scss}": [
			"stylelint --fix",
			"prettier --config .prettierrc --write"
		],
		"*.{js,jsx,ts}": [
			"node scripts/sort-exports.js",
			"eslint --fix",
			"prettier --config .prettierrc --write"
		],
		"*.{md,json,html,php}": [
			"prettier --config .prettierrc --write"
		]
	}
}
